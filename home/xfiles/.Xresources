Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
 
        URxvt --help 2>&1| sed -n '/:  /s/^ */! URxvt*/gp' >> ~/.Xresources
    URxvt*font: xft:gohufont:size=11
    URxvt.letterSpace: 1 
    URxvt.title: Terminal
    URxvt.scrollBar: false
     
    URxvt.iconFile: /usr/share/icons/Faenza/apps/scalable/terminal.svg


