!URxvt.foreground: #dcdccc
 
URxvt.saveLines:32767
URxvt.scrollBar: false


URxvt*font:xft:Terminess Powerline:pixelsize=12

!URxvt*boldFont: xft:gohufont:pixelsize=12

URxvt.perl-ext-common: default,matcher
